{
  "name": "@livestore/livestore",
  "version": "0.3.0",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/mod.d.ts",
      "default": "./dist/mod.js"
    },
    "./internal": {
      "types": "./dist/internal/mod.d.ts",
      "default": "./dist/internal/mod.js"
    },
    "./internal/testing-utils": {
      "types": "./dist/utils/tests/mod.d.ts",
      "default": "./dist/utils/tests/mod.js"
    },
    "./effect": {
      "types": "./dist/effect/mod.d.ts",
      "default": "./dist/effect/mod.js"
    }
  },
  "types": "./dist/mod.d.ts",
  "typesVersions": {
    "*": {
      "internal": [
        "./dist/internal/mod.d.ts"
      ],
      "testing-utils": [
        "./dist/utils/tests/mod.d.ts"
      ],
      "effect": [
        "./dist/effect/mod.d.ts"
      ]
    }
  },
  "dependencies": {
    "@opentelemetry/api": "1.9.0",
    "@livestore/common": "0.3.0",
    "@livestore/utils": "0.3.0"
  },
  "devDependencies": {
    "@opentelemetry/sdk-trace-base": "^2.0.0",
    "jsdom": "^26.0.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.4",
    "vitest": "^3.1.2",
    "@livestore/utils-dev": "0.3.0",
    "@livestore/adapter-web": "0.3.0"
  },
  "files": [
    "package.json",
    "src",
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "test": "vitest"
  }
}